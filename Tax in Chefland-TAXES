#include <stdio.h>

int main() 
{
	int T;
	scanf("%d",&T);
	int a[T];
	for(int i=0;i<T;i++)
	{
	    scanf("%d",&a[i]);
	}
	for(int i=0;i<T;i++)
	{
	    if(a[i]>100)
	    {
	       a[i] = a[i]-10;
	        printf("%d\n",a[i]);
	    }
	    else
	       {
	     printf("%d\n",a[i]);
        	}
	}
	return 0;
}

